<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="report_ticket">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <h2>Boarding Pass</h2>
                    Name: <t t-esc="doc.name"/>
                    Gender: <t t-esc="doc.gender"/>
                    Tanggal: <t t-esc="doc.tanggal"/>
                    Kota Keberangkatan: <t t-esc="doc.kota_keberangkatan"/>
                    Tujuan Kota: <t t-esc="doc.tujuan_kota"/>
                    Maskapai: <t t-esc="doc.maskapai"/>
                    Kelas Pesawat: <t t-esc="doc.kelas_pesawat"/>
                    Description: <t t-esc="doc.note"/>
                    
                </t> 
            </t>
        </t> 
    </template>

</odoo>